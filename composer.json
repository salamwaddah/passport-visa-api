{
	"name": "salamwaddah/passport-visa-api",
	"description": "Passport Visa API - an unofficial passportindex.org API",
	"type": "library",
	"license": "MIT",
	"keywords": [
		"passport",
		"visa",
		"index"
	],
	"authors": [
		{
			"name": "Salam Waddah",
			"email": "salamwaddah@gmail.com",
			"homepage": "https://salamwaddah.com/",
			"role": "Developer"
		}
	],
	"autoload": {
		"psr-4": {
			"SalamWaddah\\PassportVisa\\": "src/",
			"SalamWaddah\\PassportVisa\\Tests\\": "tests/"
		}
	},
	"require": {
		"ext-json": "*",
		"php": ">=7.1",
		"guzzlehttp/guzzle": "^7.8"
	},
	"minimum-stability": "stable",
	"version": "1.0.0",
	"require-dev": {
		"pestphp/pest": "^2.33"
	},
	"scripts": {
		"test": "vendor/bin/pest"
	},
	"config": {
		"allow-plugins": {
			"pestphp/pest-plugin": true
		}
	}
}
